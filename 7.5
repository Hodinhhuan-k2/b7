    with open(fname, "w") as myfile:
        myfile.write("Python Exercises\n")
        myfile.write("Java Exercises")
    
    # Mở lại file ở chế độ đọc ('r') và in ra nội dung
    with open(fname, "r") as txt:
        print(txt.read())

